{
  "name": "defi-france-ledger",
  "version": "0.0.1",
  "description": "DeFi France serveur pour sourcecred et discord",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:dev",
    "start:dev": "cross-env NODE_ENV=develop nodemon --exec babel-node src/index.js",
    "start:prod": "pm2 start ecosystem.config.js --env production",
    "build": "babel src --out-dir dist",
    "build:remove": "del dist",
    "log:remove": "del log",
    "serve": "npm run start:prod",
    "test": "npm run test:eslint && npm run test:flow && npm run test:e2e",
    "test:light": "npm run test:eslint && npm run test:flow",
    "test:eslint": "eslint src/",
    "test:eslint-fix": "eslint src/ --fix",
    "test:flow": "flow src",
    "test:e2e": "cross-env NODE_ENV=test babel-node e2e/index.js"
  },
  "author": "Nolan Vanmoortel",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "compression": "^1.7.4",
    "config": "^3.2.4",
    "cors": "^2.8.5",
    "elasticsearch": "^16.5.0",
    "express": "^4.17.1",
    "express-winston": "^4.0.1",
    "jsome": "^2.5.0",
    "moment": "^2.24.0",
    "mongodb": "^3.3.3",
    "mongodb-memory-server": "^6.0.1",
    "spdy": "^4.0.1",
    "winston": "^3.2.1",
    "winston-elasticsearch": "^0.8.2",
    "winston-transport": "^4.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.0",
    "@babel/core": "^7.7.2",
    "@babel/node": "^7.7.0",
    "@babel/preset-flow": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-preset-airbnb": "^4.1.0",
    "cross-env": "^6.0.3",
    "del-cli": "^3.0.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "flow-bin": "^0.111.3",
    "nodemon": "^1.19.4"
  }
}
